package co.edureka.packa;

class CA {
	
	private void awesome(){
		System.out.println("This is in awesome of CA");
	}
	
	void showA(){
		System.out.println("This is in show of CA");
	}
	
	public void helloA(){
		System.out.println("This is in hello of CA");
	}
	
	protected void thanks(){
		System.out.println("This is in thanks of CA and is protected");
	}
}
