package co.edureka;

class Thread1 extends Thread{
	
	@Override
	public void run() {
		for(int i=1;i<=10;i++){
			System.out.println("**Thread1**");
		}
	}
}

public class Exe {
	// Main Thread
	public static void main(String[] args) {	
		System.out.println("Main Thread Started");
		
		for(int i=1;i<=10;i++){
			System.out.println("--Main Thread--");
		}
		
		
		System.out.println("Main Thread Finished");
	}
}
